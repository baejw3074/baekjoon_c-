#include <stdio.h>
#include <stdlib.h>
 
 
int main(int argc, char const *argv[]) {
    int A;
    char B[4];
 
    scanf("%d", &A);
    scanf("%s", B);
 
    printf("%d\n", A * (B[2] - '0'));
    printf("%d\n", A * (B[1] - '0'));
    printf("%d\n", A * (B[0] - '0'));
    printf("%d\n", A * atoi(B));
    return 0;
}
